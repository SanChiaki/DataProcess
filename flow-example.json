{
    "id": "flow_001",
    "name": "Excel Sheet合并流程",
    "description": "合并Excel的两个Sheet页",
    "nodes": [
      {
        "id": "node_001",
        "name": "Sheet1输入",
        "type": "EXCEL_INPUT",
        "version": "1.0.0",
        "config": {
          "filePath": "test.xlsx",
          "sheetName": "Sheet1",
          "range": "A1:D100"
        }
      },
      {
        "id": "node_002",
        "name": "Sheet2输入",
        "type": "EXCEL_INPUT",
        "version": "1.0.0",
        "config": {
          "filePath": "test.xlsx",
          "sheetName": "Sheet2",
          "range": "A1:D100"
        }
      },
      {
        "id": "node_003",
        "name": "Sheet合并",
        "type": "EXCEL_SHEET_MERGE",
        "version": "1.0.0",
        "config": {
          "mergeType": "UNION"
        }
      },
      {
        "id": "node_004",
        "name": "结果输出",
        "type": "EXCEL_OUTPUT",
        "version": "1.0.0",
        "config": {
          "filePath": "result.xlsx",
          "sheetName": "合并结果"
        }
      }
    ],
    "edges": [
      {
        "sourceId": "node_001",
        "targetId": "node_003",
        "sourcePort": {
          "id": "out_001",
          "type": "OUTPUT"
        },
        "targetPort": {
          "id": "in_001",
          "type": "INPUT"
        }
      },
      {
        "sourceId": "node_002",
        "targetId": "node_003",
        "sourcePort": {
          "id": "out_001",
          "type": "OUTPUT"
        },
        "targetPort": {
          "id": "in_002",
          "type": "INPUT"
        }
      },
      {
        "sourceId": "node_003",
        "targetId": "node_004",
        "sourcePort": {
          "id": "out_001",
          "type": "OUTPUT"
        },
        "targetPort": {
          "id": "in_001",
          "type": "INPUT"
        }
      }
    ]
  }